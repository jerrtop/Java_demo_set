                <goals>
                  <goal>unpack-dependencies</goal>
                </goals>
                <configuration>
                  <includeGroupIds>org.apache.cxf</includeGroupIds>
                  <classifier>sources</classifier>
                  <outputDirectory>/home/dkulp/working/cxf-trunk/target/checkout/osgi/bundle/all/target/sources</outputDirectory>
                </configuration>
              </execution>
              <execution>
                <id>unpack-plugin-sources</id>
                <phase>generate-sources</phase>
                <goals>
                  <goal>unpack</goal>
                </goals>
                <configuration>
                  <artifactItems>
