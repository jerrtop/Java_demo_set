                      <groupId>org.apache.cxf</groupId>
                      <artifactId>cxf-codegen-plugin</artifactId>
                      <version>2.7.0</version>
                      <classifier>sources</classifier>
                    </artifactItem>
                  </artifactItems>
                  <outputDirectory>/home/dkulp/working/cxf-trunk/target/checkout/osgi/bundle/all/target/sources</outputDirectory>
                </configuration>
              </execution>
            </executions>
